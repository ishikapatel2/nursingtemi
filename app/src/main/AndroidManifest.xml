<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.nursingtemi"
    >

    <application

        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Nursingtemi"
        tools:targetApi="31">

        <meta-data
            android:name="com.robotemi.sdk.metadata.SKILL"
            android:value="@string/app_name" />
        <activity
            android:name=".screens.ConfirmMessageActivity"
            android:exported="false"
            android:parentActivityName=".screens.MainActivity"/>
        <activity
            android:name=".screens.InfoActivity"
            android:exported="false"
            android:parentActivityName=".screens.MainActivity"/>
        <activity
            android:name=".screens.SurveyActivity"
            android:exported="false"
            android:parentActivityName=".screens.MainActivity"/>
        <activity
            android:name=".screens.DeliveryActivity"
            android:exported="false"
            android:parentActivityName=".screens.MainActivity"/>
        <activity
            android:name=".screens.TourActivity"
            android:exported="false"
            android:parentActivityName=".screens.MainActivity"/>
        <activity
            android:name=".screens.DeliveryContinuationActivity"
            android:exported="false"
            android:parentActivityName=".screens.MainActivity"/>
        <activity
            android:name=".screens.MainActivity"
            android:exported="false" />
        <activity
            android:name=".screens.BeginningActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>